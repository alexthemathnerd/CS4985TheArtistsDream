<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefWidth="200.0" style="-fx-border-color: #1e1e24; -fx-background-color: #1e1e24;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="CENTER" style="-fx-font-weight: BOLD;" text="Icon" textAlignment="CENTER" textFill="#e01a4f" wrapText="true">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ImageView fx:id="artworkImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
         <image>
            <Image url="@CS4985TheArtistsDream/code/TheArtistsDreamClient/src/main/resources/edu/westga/devops/theartistsdreamclient/icon.png" />
         </image></ImageView>
   </children>
</VBox>
